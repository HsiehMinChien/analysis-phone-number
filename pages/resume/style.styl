.resume
  height: 100vh
  background-color: #eee

  &.dark-mode
    background-color: #252525
    .setting-icon
      opacity: 0.4

  .setting-icon
    position: fixed
    bottom: 90px
    right: 20px
    width: 60px
    height: 60px
    text-align: center
    border-radius: 50%
    background-color: #ffffff
    opacity: 0.8

    span
      position: relative
      top: 10px
      font-size: 40px
